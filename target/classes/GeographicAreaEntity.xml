<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="entity.GeographicAreaEntity">
        <table name="geographicarea" schema="canadacensusdb" catalog=""/>
        <attributes>
            <id name="geographicAreaID">
                <column name="geographicAreaID" nullable="false"/> 
            </id>
            <basic name="code">
                <column name="code" nullable="false"/>
            </basic>
            <basic name="level">
                <column name="level" nullable="false"/>
            </basic>
            <basic name="name">
                <column name="name" length="40" nullable="false"/>
            </basic>
            <basic name="alternativeCode">
                <column name="alternativeCode" nullable="false"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
